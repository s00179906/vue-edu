{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["plugin:vue/essential", "eslint:recommended", "@vue/prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "arrow-parens": "off",
    "no-return-assign": "off",
    "no-console": "off",
    "no-unused-expressions": "warn",
    "linebreak-style": "off",
    "no-undef": "off",
    "no-unused-vars": "off"
  }
}
